<div id="radio">
    <pe-radio 
        :text='radio.label'
        :id='radio.ID'
        :classes='radio.class'
        :formvalue='radio.value'
        formname='radio-form'
        >
    </pe-radio>
    <pe-radio 
        :text='radio2.label'
        :id='radio2.ID'
        :classes='radio2.class'
        :formvalue='radio2.value'
        formname='radio-form'
        >
    </pe-radio>
    <!--\{{selected}}-->
</div>

<script>
var radio = {{{json radio}}};
var radio2 = {{{json radio2}}};

requirejs(['vue', 'atoms/Radio'], function (Vue, Radio) {
    console.log(Radio.default);
    new Vue({
        el: "#radio",
        data: function () {
            return {
                selected: [], 
                radio: radio,
                radio2: radio2
            }
        },
        components: {
            'pe-radio': Radio.default
        }
    });
});
</script>